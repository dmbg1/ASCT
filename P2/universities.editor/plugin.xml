<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated universities -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="universities.presentation.UniversitiesModelWizardID"
            name="%_UI_UniversitiesModelWizard_label"
            class="universities.presentation.UniversitiesModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/UniversitiesModelFile.gif">
         <description>%_UI_UniversitiesModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated universities -->
      <editor
            id="universities.presentation.UniversitiesEditorID"
            name="%_UI_UniversitiesEditor_label"
            icon="icons/full/obj16/UniversitiesModelFile.gif"
            extensions="universities"
            class="universities.presentation.UniversitiesEditor"
            contributorClass="universities.presentation.UniversitiesActionBarContributor">
      </editor>
   </extension>

</plugin>
